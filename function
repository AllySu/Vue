const App = { 
setup() { 
// data 
const count = value(0) 
// computed 
const plusOne = computed(() => count.value + 1)
 // method 
const increment = () => { count.value++ } 
// watch
 watch(() => count.value * 2, v => console.log(v)) 
// lifecycle 
onMounted(() => console.log('mounted!')) 
// 暴露给模版或渲染函数 
return { count } 
 } 
}
